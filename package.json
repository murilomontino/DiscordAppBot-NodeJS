{
  "name": "botapp",
  "version": "0.2.0",
  "author": "Muca e Luke",
  "build": {
    "appId": "com.discord_cthulhu_bot.app",
    "productName": "Cthulhu App",
    "copyright": "Copyright Â© 2019 Banhistas Porcos Selvagens",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "src/assets/cons"
    }
  },
  "private": true,
  "homepage": ".",
  "main": "./public/electron.js",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@unform/core": "^2.1.3",
    "@unform/web": "^2.1.3",
    "d20": "1.4.1",
    "discord.js": "12.3.1",
    "electron-is-dev": "^1.2.0",
    "electron-store": "^6.0.0",
    "fontsource-roboto": "^3.0.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "fix": "./node_modules/.bin/eslint .  --fix",
    "dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && yarn nodemon  .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-build": "yarn build",
    "electron-build": "electron-builder -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.14",
    "@rescripts/rescript-env": "^0.0.12",
    "concurrently": "5.3.0",
    "cross-env": "^7.0.2",
    "electron": "10.1.0",
    "electron-builder": "^22.8.1",
    "nodemon": "^2.0.4",
    "typescript": "^4.0.3",
    "wait-on": "^5.2.0",
    "webpack-cli": "^3.3.12"
  }
}
